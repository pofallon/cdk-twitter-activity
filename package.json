{
  "name": "cdk-twitter-activity",
  "version": "1.0.0",
  "description": "AWS CDK construct to publish Twitter activity to AWS EventBridge",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "excludeTypescript": [
      "examples",
      "test"
    ],
    "targets": {
      "dotnet": {
        "namespace": "Get2Know.CDK.TwitterActivity",
        "packageId": "Get2Know.CDK.TwitterActivity"
      }
    }
  },
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "clean": "rimraf dist lib/*.js lib/*.d.ts tsconfig.json .parcel-cache",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pofallon/cdk-twitter-activity.git"
  },
  "keywords": [
    "cdk",
    "twitter",
    "eventbridge"
  ],
  "author": "Paul O'Fallon (https://www.get2know.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pofallon/cdk-twitter-activity/issues"
  },
  "homepage": "https://github.com/pofallon/cdk-twitter-activity#readme",
  "devDependencies": {
    "@aws-cdk/assert": "1.73.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "aws-sdk": "^2.792.0",
    "jest": "^26.6.3",
    "jsii": "^1.14.1",
    "jsii-pacmak": "^1.14.1",
    "jsii-release": "^0.2.3",
    "parcel": "2.0.0-beta.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "@aws-cdk/core": "1.73.0",
    "@aws-cdk/aws-apigatewayv2": "^1.73.0",
    "@aws-cdk/aws-cloudformation": "^1.73.0",
    "@aws-cdk/aws-events": "^1.73.0",
    "@aws-cdk/aws-lambda": "^1.73.0",
    "@aws-cdk/custom-resources": "^1.73.0",
    "constructs": "^3.2.27",
    "@aws-cdk/aws-lambda-nodejs": "^1.73.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.73.0"
  },
  "bundleDependencies": [
    "range_check",
    "twttr"
  ],
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.73.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.73.0",
    "@aws-cdk/aws-cloudformation": "^1.73.0",
    "@aws-cdk/aws-events": "^1.73.0",
    "@aws-cdk/aws-lambda": "^1.73.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.73.0",
    "@aws-cdk/core": "1.73.0",
    "@aws-cdk/custom-resources": "^1.73.0",
    "range_check": "^2.0.3",
    "twttr": "^2.0.1"
  },
  "targets": {
    "cdk-lambda": {
      "context": "node",
      "includeNodeModules": {
        "aws-sdk": false
      },
      "sourceMap": false,
      "minify": false,
      "engines": {
        "node": ">= 12"
      }
    }
  }
}