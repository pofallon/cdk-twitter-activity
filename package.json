{
  "name": "cdk-twitter-activity",
  "version": "1.0.0",
  "description": "AWS CDK construct to publish Twitter activity to AWS EventBridge",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "excludeTypescript": [
      "examples",
      "test"
    ],
    "targets": {
      "dotnet": {
        "namespace": "Get2Know.CDK.TwitterActivity",
        "packageId": "Get2Know.CDK.TwitterActivity"
      }
    }
  },
  "scripts": {
    "prebuild": "webpack",
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "clean": "rimraf dist lambda-dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pofallon/cdk-twitter-activity.git"
  },
  "keywords": [
    "cdk",
    "twitter",
    "eventbridge"
  ],
  "author": "Paul O'Fallon (https://www.get2know.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pofallon/cdk-twitter-activity/issues"
  },
  "homepage": "https://github.com/pofallon/cdk-twitter-activity#readme",
  "devDependencies": {
    "@aws-cdk/assert": "1.57.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "aws-sdk": "^2.731.0",
    "ip-cidr": "^2.0.17",
    "jest": "^26.3.0",
    "jsii": "^1.10.0",
    "jsii-pacmak": "^1.10.0",
    "jsii-release": "^0.1.9",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "twttr": "^2.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "@aws-cdk/core": "1.57.0",
    "@aws-cdk/aws-apigatewayv2": "^1.57.0",
    "@aws-cdk/aws-cloudformation": "^1.57.0",
    "@aws-cdk/aws-events": "^1.57.0",
    "@aws-cdk/aws-lambda": "^1.57.0",
    "@aws-cdk/custom-resources": "^1.57.0",
    "constructs": "^3.0.4"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.57.0",
    "@aws-cdk/aws-cloudformation": "^1.57.0",
    "@aws-cdk/aws-events": "^1.57.0",
    "@aws-cdk/aws-lambda": "^1.57.0",
    "@aws-cdk/custom-resources": "^1.57.0",
    "@aws-cdk/core": "1.57.0"
  }
}
