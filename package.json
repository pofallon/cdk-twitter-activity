{
  "name": "twitter-activity-cdk",
  "version": "1.0.0",
  "description": "AWS CDK construct to publish Twitter activity to AWS EventBridge",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "excludeTypescript": [
      "examples"
    ],
    "targets": {
      "dotnet": {
        "namespace": "Get2Know.CDK.TwitterActivity",
        "packageId": "Get2Know.CDK.TwitterActivity"
      }
    }
  },
  "scripts": {
    "prebuild": "webpack",
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "clean": "rimraf dist lambda-dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pofallon/twitter-activity-cdk.git"
  },
  "keywords": [
    "cdk",
    "twitter",
    "eventbridge"
  ],
  "author": "Paul O'Fallon (https://www.get2know.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pofallon/twitter-activity-cdk/issues"
  },
  "homepage": "https://github.com/pofallon/twitter-activity-cdk#readme",
  "devDependencies": {
    "@aws-cdk/assert": "1.44.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "aws-sdk": "^2.690.0",
    "ip-cidr": "^2.0.12",
    "jest": "^26.0.1",
    "jsii": "^1.6.0",
    "jsii-pacmak": "^1.6.0",
    "jsii-release": "^0.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "twttr": "^1.0.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "@aws-cdk/core": "1.44.0",
    "@aws-cdk/aws-apigatewayv2": "^1.44.0",
    "@aws-cdk/aws-cloudformation": "^1.44.0",
    "@aws-cdk/aws-events": "^1.44.0",
    "@aws-cdk/aws-lambda": "^1.44.0",
    "@aws-cdk/custom-resources": "^1.44.0",
    "constructs": "^3.0.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.44.0",
    "@aws-cdk/aws-cloudformation": "^1.44.0",
    "@aws-cdk/aws-events": "^1.44.0",
    "@aws-cdk/aws-lambda": "^1.44.0",
    "@aws-cdk/core": "1.44.0",
    "@aws-cdk/custom-resources": "^1.44.0"
  }
}
